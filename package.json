{
  "name": "portfolio",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/styles/smui.css -i src/theme/light",
    "smui-theme-dark": "smui-theme compile static/styles/smui-dark.css -i src/theme/dark",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write ./src"
  },
  "devDependencies": {
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@mdi/js": "^7.2.96",
    "@smui/button": "^7.0.0-beta.8",
    "@smui/card": "^7.0.0-beta.8",
    "@smui/data-table": "^6.2.0",
    "@smui/dialog": "^7.0.0-beta.8",
    "@smui/drawer": "^7.0.0-beta.8",
    "@smui/fab": "^7.0.0-beta.8",
    "@smui/icon-button": "^7.0.0-beta.8",
    "@smui/layout-grid": "^7.0.0-beta.8",
    "@smui/linear-progress": "^7.0.0-beta.8",
    "@smui/list": "^7.0.0-beta.8",
    "@smui/segmented-button": "^7.0.0-beta.8",
    "@smui/select": "^7.0.0-beta.8",
    "@smui/snackbar": "^7.0.0-beta.8",
    "@smui/tab": "^7.0.0-beta.8",
    "@smui/tab-bar": "^7.0.0-beta.8",
    "@smui/textfield": "^7.0.0-beta.8",
    "@smui/top-app-bar": "^7.0.0-beta.8",
    "@sveltejs/adapter-auto": "^2.0.1",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.15.11",
    "@tsconfig/strictest": "^2.0.1",
    "@types/lodash": "^4.14.194",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "lodash": "^4.17.21",
    "newt-client-js": "^3.2.4",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "smui-theme": "^7.0.0-beta.8",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-material-icons": "^3.0.4",
    "svelte-meta-tags": "^2.7.0",
    "svelte-preprocess": "^5.0.3",
    "svelte-scrollto": "^0.2.0",
    "tslib": "^2.5.0",
    "typescript": "*",
    "vite": "^4.3.4",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.31.0",
    "eslint-plugin-svelte": "^2.27.3"
  }
